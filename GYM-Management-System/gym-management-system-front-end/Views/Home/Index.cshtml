@{
    ViewData["Title"] = "Home Page";
 
}

<header class="masthead vh-100 min-vh-100 ">
    <div class="container">
        <div class="masthead-subheading">Welcome To Our Gym!</div>
        <div class="masthead-heading text-uppercase">Are you ready?</div>
        <a class="btn btn-primary btn-xl text-uppercase" href="#services">Tell Me More</a>
    </div>
</header>
<section class="page-section" id="Gyms">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">Our Branshes</h2>
            <h3 class="section-subheading text-muted">Check the Branches</h3>
            @await Component.InvokeAsync("GetAllGyms")
        </div>
    </div>
</section>
<section class="page-section bg-dark" id="Supplements">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">Our Supplements</h2>
            <h3 class="section-subheading text-muted">Check the Supplements</h3>
            @await Component.InvokeAsync("GetAllSupplements")
        </div>
    </div>
</section>
<partial name="_TeamSection"/>
<partial name="_ContactView" />

