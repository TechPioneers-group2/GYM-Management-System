
@model IEnumerable<gym_management_system_front_end.Models.SupplementViewModel>
@using Microsoft.AspNetCore.Mvc
@using Microsoft.AspNetCore.Html
@using Newtonsoft.Json
@using System.Net.Http
@using System.Web

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4 mb-4 supplementscard">
            <div class="card">
                <img src="@item.imageURL" alt="...">
                <div class="card-body">
                    <h5 class="card-title text-dark fw-bold">@item.Name</h5>
                    <p class="text-dark fw-bold">SEE MORE</p>
                </div>
               
            </div>
        </div>
    }
</div>



