@model IEnumerable<gym_management_system_front_end.Models.SupplementViewModel>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4 mb-4 supplementscard">
            <div class="card">
                <img src="https://cdn.pixabay.com/photo/2015/01/10/18/45/gym-595597_1280.jpg" class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title text-dark fw-bold">@item.Name</h5>
                    <p class="text-dark fw-bold">SEE MORE</p>
                </div>
            </div>
        </div>
    }
</div>



@*Commint*@

